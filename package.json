{
  "name": "asbestosguard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.json",
    "preview": "vite preview",
    "server": "tsx server.ts",
    "agent-bridge": "python agent-bridge-service.py",
    "agent-bridge:mock": "python mock-bridge-service.py",
    "start": "node dist-server/server.js",
    "start:dev": "npm-run-all --parallel dev server",
    "start:prod": "node dist-server/server.js",
    "start:with-bridge": "npm-run-all --parallel agent-bridge server dev",
    "start:with-mock-bridge": "npm-run-all --parallel agent-bridge:mock server dev",
    "test:foundry": "tsx tools/test-foundry-agent.ts",
    "test:foundry-analysis": "tsx test-foundry-analysis.ts",
    "test:foundry-analysis:mock": "npm-run-all --parallel agent-bridge:mock test:foundry-analysis-noserver",
    "test:foundry-analysis-noserver": "tsx test-foundry-analysis.ts",
    "test:agent-lookup": "tsx test-agent-lookup.ts",
    "discover:agents": "tsx tools/discover-foundry-agents.ts",
    "get:agent-id": "tsx tools/get-agent-id.ts",
    "validate:setup": "tsx validate-foundry-setup.ts"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/monitor-opentelemetry": "^1.14.2",
    "@azure/storage-blob": "^12.29.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "ajv": "^8.17.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "lucide-react": "^0.559.0",
    "mammoth": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
